{
  "name": "@activityrank/web",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@4.1.0",
  "scripts": {
    "dev": "DATABASE_URL=.dev.db remix vite:dev",
    "build": "remix vite:build",
    "start": "remix-serve ./build/server/index.js",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "format": "prettier --write --log-level warn .",
    "format:check": "prettier --check .",
    "typecheck": "tsc",
    "db:rebuild": "[ ! -e .dev.db ] || rm .dev.db && cat schema.sql | sqlite3 .dev.db",
    "db:codegen": "kysely-codegen --url .dev.db --dialect sqlite",
    "db": "yarn db:rebuild && yarn db:codegen"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@ltd/j-toml": "^1.38.0",
    "@lucia-auth/adapter-sqlite": "^3.0.0",
    "@remix-run/node": "^2.8.0",
    "@remix-run/react": "^2.8.0",
    "@remix-run/serve": "^2.8.0",
    "arctic": "^1.2.1",
    "better-sqlite3": "^9.4.3",
    "clsx": "^2.1.0",
    "esbuild": "^0.20.1",
    "framer-motion": "^11.0.8",
    "isbot": "^4.4.0",
    "kysely": "^0.27.2",
    "lucia": "^3.0.1",
    "mdx-bundler": "^10.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-flat-routes": "^0.6.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.8.0",
    "@types/better-sqlite3": "^7.6.9",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "autoprefixer": "^10.4.18",
    "discord-api-types": "^0.37.71",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "kysely-codegen": "^0.11.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-imagetools": "^6.2.9",
    "vite-tsconfig-paths": "^4.3.1"
  }
}
